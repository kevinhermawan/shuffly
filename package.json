{
  "type": "module",
  "name": "shuffly",
  "description": "A JavaScript utility for shuffling arrays",
  "version": "1.0.4",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rimraf dist && rollup -c",
    "format": "biome format . --write",
    "format:check": "biome format .",
    "lint": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "1.0.0",
    "@vitest/coverage-v8": "^0.34.3",
    "esbuild": "^0.19.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Kevin Hermawan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinhermawan/shuffly.git"
  },
  "bugs": {
    "url": "https://github.com/kevinhermawan/shuffly/issues"
  },
  "homepage": "https://github.com/kevinhermawan/shuffly#readme",
  "keywords": [
    "shuffle",
    "random",
    "permutation",
    "fisher-yates",
    "knuth-shuffle",
    "sattolo"
  ]
}
